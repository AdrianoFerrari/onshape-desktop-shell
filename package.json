{
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "node build/install-production-deps.js",
    "start": "npm install && npm run compile && electron ./app",
    "clean": "rimraf dist",
    "clean:osx": "rimraf dist/Onshape-darwin-x64",
    "clean:win": "rimraf dist/win",
    "compile": "rimraf app/out && tsc -p src",
    "pack:osx": "npm run compile && node build/build.js --platform=darwin",
    "pack:win": "npm run compile && node build/build.js --platform=win32",
    "pack": "npm run pack:osx && npm run pack:win",
    "build:osx": "npm run compile && node build/build.js --build --platform=darwin --sign='Vladimir Krivosheev'",
    "build:win": "npm run compile && node build/build.js --build --platform=win32",
    "build": "npm run build:osx && npm run build:win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/onshape-desktop-shell.git"
  },
  "devDependencies": {
    "command-line-args": "^2.1.3",
    "electron-builder": "^2.5.0",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.2",
    "rimraf": "^2.5.0",
    "typescript": "^1.8.0-dev.20151225"
  }
}
